    "http-call": "^5.3.0",
    "moment-timezone": "^0.5.33"
  },
  "devDependencies": {
    "@heroku-cli/schema": "^1.0.25",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.21",
